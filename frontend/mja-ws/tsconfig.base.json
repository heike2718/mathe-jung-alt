{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "module": "esnext",
    "lib": ["es2017", "dom"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@mja-ws/core/api": ["libs/core/api/src/index.ts"],
      "@mja-ws/core/data": ["libs/core/data/src/index.ts"],
      "@mja-ws/core/model": ["libs/core/model/src/index.ts"],
      "@mja-ws/deskriptoren/model": ["libs/deskriptoren/model/src/index.ts"],
      "@mja-ws/embeddable-images/api": [
        "libs/shared/embeddable-images/api/src/index.ts"
      ],
      "@mja-ws/embeddable-images/data": [
        "libs/shared/embeddable-images/data/src/index.ts"
      ],
      "@mja-ws/embeddable-images/model": [
        "libs/shared/embeddable-images/model/src/index.ts"
      ],
      "@mja-ws/local-storage-data": ["libs/local-storage-data/src/index.ts"],
      "@mja-ws/raetsel/api": ["libs/raetsel/api/src/index.ts"],
      "@mja-ws/raetsel/data": ["libs/raetsel/data/src/index.ts"],
      "@mja-ws/raetsel/feature": ["libs/raetsel/feature/src/index.ts"],
      "@mja-ws/raetsel/model": ["libs/raetsel/model/src/index.ts"],
      "@mja-ws/aufgabensammlungen/api": ["libs/aufgabensammlungen/api/src/index.ts"],
      "@mja-ws/aufgabensammlungen/data": ["libs/aufgabensammlungen/data/src/index.ts"],
      "@mja-ws/aufgabensammlungen/feature": [
        "libs/aufgabensammlungen/feature/src/index.ts"
      ],
      "@mja-ws/aufgabensammlungen/model": [
        "libs/aufgabensammlungen/model/src/index.ts"
      ],
      "@mja-ws/shared/components": ["libs/shared/components/src/index.ts"],
      "@mja-ws/shared/config": ["libs/shared/config/src/index.ts"],
      "@mja-ws/shared/http": ["libs/shared/http/src/index.ts"],
      "@mja-ws/shared/layout": ["libs/shared/layout/src/index.ts"],
      "@mja-ws/shared/messaging/api": [
        "libs/shared/messaging/api/src/index.ts"
      ],
      "@mja-ws/shared/messaging/ui": ["libs/shared/messaging/ui/src/index.ts"],
      "@mja-ws/shared/ngrx-utils": ["libs/shared/ngrx-utils/src/index.ts"],
      "@mja-ws/shared/util": ["libs/shared/util/src/index.ts"]
    }
  },
  "exclude": ["node_modules", "tmp"]
}
