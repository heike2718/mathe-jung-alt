{
  "name": "embeddable-images-api",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/shared/embeddable-images/api/src",
  "prefix": "mja-ws",
  "tags": ["domain:shared:embeddable-images:api", "type:api"],
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/angular:ng-packagr-lite",
      "outputs": ["{workspaceRoot}/dist/{projectRoot}"],
      "options": {
        "project": "libs/shared/embeddable-images/api/ng-package.json"
      },
      "configurations": {
        "production": {
          "tsConfig": "libs/shared/embeddable-images/api/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "libs/shared/embeddable-images/api/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/shared/embeddable-images/api/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "libs/shared/embeddable-images/api/**/*.ts",
          "libs/shared/embeddable-images/api/**/*.html",
          "libs/shared/embeddable-images/api/package.json"
        ]
      }
    }
  }
}
