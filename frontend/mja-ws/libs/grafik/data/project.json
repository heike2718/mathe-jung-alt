{
  "name": "grafik-data",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/grafik/data/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/grafik/data",
        "main": "libs/grafik/data/src/index.ts",
        "tsConfig": "libs/grafik/data/tsconfig.lib.json",
        "assets": ["libs/grafik/data/*.md"],
        "updateBuildableProjectDepsInPackageJson": true
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/grafik/data/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/libs/grafik/data"],
      "options": {
        "jestConfig": "libs/grafik/data/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": ["domain:grafik:data", "type:data"]
}
