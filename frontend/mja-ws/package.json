  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js",
    "start": "nx serve",
    "build:mja-app:local": "npx nx build mja-app -c local --output-hashing=all --base-href /mja-app/ --skip-nx-cache",
    "build:mja-app:a300": "npx nx build mja-app -c a300 --output-hashing=all --base-href /mja-app/ --skip-nx-cache",
    "build:mja-app:prod": "npx nx run mja-app:build:production --output-hashing=all --base-href /mja-app/ --skip-nx-cache",
    "test": "npx nx test",
    "test:all": "npx nx run-many --target=test --all",
    "lint:all": "npx nx run-many --target=lint --all",
    "full-check": "npx nx run-s lint:all test:all build",
    "graph": "npx nx graph"
  },
